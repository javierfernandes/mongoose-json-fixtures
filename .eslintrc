{
  "parser": "babel-eslint",
  "extends": ["airbnb-base"],
  "rules": {
    "comma-dangle": [
      2,
      "never"
    ],
    "semi": 0,
    "max-len": 0,
    "no-multiple-empty-lines": 0,
    "space-before-function-paren": 0,
    "object-shorthand": 0,
    "padded-blocks": 0,
    "eol-last": 0,
    "arrow-parens": 0,
    "import/first": 0,
    "prefer-template" : "warn",
    "no-underscore-dangle": 0,
    "import/no-dynamic-require": 0,
    "import/prefer-default-export": 0,
    "global-require": 0,
    "no-use-before-define" : 0,
    "consistent-return": 0,
    "func-names": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "object-curly-spacing": "warn",
    "radix": 0,
    "indent": "warn"
  }
}

